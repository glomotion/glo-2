{
  "name": "glo-os",
  "version": "1.0.0",
  "description": "Glo version 2.0",
  "author": "Tim Paul <tim@glo.id.au>",
  "scripts": {
    "prestart": "yarn clean",
    "start": "parcel -p 8080 --open",
    "e2e": "node test/e2e/runner.js",
    "test": "yarn run e2e",
    "lint": "eslint --ext .js,.vue src test/e2e/specs",
    "clean": "rm -rf dist/",
    "prebuild": "yarn clean",
    "build": "parcel build index.html",
    "presurge": "yarn build; cp CNAME-src dist/CNAME;",
    "surge": "surge dist/"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.0.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "parcel-bundler": "^1.7.0",
    "parcel-plugin-vue": "^1.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import-url": "^3.0.4",
    "postcss-normalize": "^4.0.0",
    "surge": "^0.20.1",
    "vue-template-compiler": "^2.5.16"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
